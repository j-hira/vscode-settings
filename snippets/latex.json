{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"verb": {
		"prefix": "@verb",
		"body": [
			"\\verb|$1|"
		]
	},
	"verbatim": {
		"prefix": "@verbatim",
		"body": [
			"\\begin{verbatim}",
			"    $1$0",
			"\\end{verbatim}",
		]
	},
	"figure": {
		"prefix": "@figure",
		"body": [
			"\\begin{figure}[H]",
			"    \\centering",
			"    \\includegraphics[width=17cm]{$1}",
			"    \\caption{$2}",
			"    \\label{$3}",
			"\\end{figure}",
		]
	},
	"itemize": {
		"prefix": "@itemize",
		"body": [
			"\\begin{itemize}",
			"    \\item $1$0",
			"\\end{itemize}",
		]
	},
	"bibitem": {
		"prefix": "@bibitem",
		"body": [
			"\\bibitem{$1} $2 \\url{$3} (閲覧日:$4月$5日)",
		]
	},
	"section-jikken": {
		"prefix": "@section-jikken",
		"body": [
			"\\section{$1}",
			"\\subsection{目的・概要}",
			"\\subsection{実験方法}",
			"\\subsection{実験結果}",
			"\\subsection{考察}",
		]
	},
	"enumerate": {
		"prefix": "@enumerate",
		"body": [
			"\\begin{enumerate}",
			"    \\item $1$0",
			"\\end{enumerate}",
		]
	},
	"section": {
		"prefix": "@section",
		"body": [
			"\\section{$1}",
		]
	},
	"subsection": {
		"prefix": "@subsection",
		"body": [
			"\\subsection{$1}",
		]
	},
	"url": {
		"prefix": "@url",
		"body": [
			"\\url{$1}",
		]
	},
	"comment": {
		"prefix": "@comment",
		"body": [
			"\\begin{comment}",
			"    $1$0",
			"\\end{comment}",
		]
	},
	"lstlisting": {
		"prefix": "@lstlisting",
		"body": [
			"\\begin{lstlisting}[caption=$1]$0",
			"\\end{lstlisting}",
		]
	},
}
